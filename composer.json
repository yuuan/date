{
    "name": "yuuan/date",
    "description": "Representation of date or month",
    "type": "library",
    "homepage": "https://github.com/yuuan/date",
    "authors": [
        {
            "name": "yuuAn",
            "homepage": "https://github.com/yuuan"
        }
    ],
    "require": {
        "php": ">=7.4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/phpstan": "^0.12.81",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "fix": "php-cs-fixer fix",
        "lint": "php-cs-fixer fix --dry-run"
    },
    "autoload": {
        "psr-4": {
            "Yuuan\\Date\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuuan\\Tests\\Date\\": "tests/"
        }
    },
    "license": "MIT"
}
